{
  "name": "Gather",
  "description": "Senior Design Project",
  "version": "0.0.1",
  "manifest_version": 3,

  "background": {
    "service_worker": "./background.js"
  },
  "action": {
    "default_popup": "./popup.html"
  },

  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
